  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!--配置程序集与命名空间的名称-->
    <add key="AssemblyPath" value="BAR.TeamManager.DAL" />
    <add key="NameSpace" value="BAR.TeamManager.DAL" />
    <!--盘古分词-->
    <add key="lucenedirPath" value="C:\lucenedir"/>
    <!--图片路径-->
    <add key="honorSlider" value="/img/slider/honorSlider"/>
